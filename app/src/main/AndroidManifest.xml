<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.restfood.myapplication">

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".OrderHistory"></activity>
        <activity android:name=".MinInventory" />
        <activity android:name=".DetailInventory" />
        <activity android:name=".Summary" />
        <activity android:name=".Inventry" />
        <activity android:name=".Qr" />
        <activity android:name=".SignInActivity" />
        <activity android:name=".EditFood" />
        <activity android:name=".EditFoodList" />
        <activity android:name=".AddCategoryList" />
        <activity android:name=".AddCategory" />
        <activity android:name=".EditProfile" />
        <activity android:name=".VerifyPhone" />
        <activity android:name=".Add_FoodItem" />
        <activity
            android:name=".main_bottom_navigation"
            android:label="@string/title_activity_main_bottom_navigation" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>